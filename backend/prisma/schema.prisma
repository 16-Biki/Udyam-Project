generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Registration {
  id        Int      @id @default(autoincrement())
  aadhaar   String
  pan       String
  otp       String?
  fullName  String?
  email     String?
  phone     String?
  createdAt DateTime @default(now())
}
